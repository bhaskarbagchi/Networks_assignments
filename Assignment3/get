xdotool search --onlyvisible --name okular windowactivate
wid=`xdotool search --onlyvisible --name okular`

echo $wid

eval `xdotool getwindowgeometry --shell $wid`
echo $X $Y
echo $WIDTH $HEIGHT

#xdotool key --delay 250 ctrl+g ctrl+c

xdotool key --delay 250 ctrl+f Escape ctrl+f Tab Tab Tab Tab Tab 1 5 0 Return Escape

xdotool getactivewindow windowsize 600 600



#  xdotool windowmove $wid $X $Y
